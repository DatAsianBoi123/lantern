native fun print(x: std.str);
native fun int_to_str(int: std.int) -> std.str;
native fun input_int() -> std.int;

val index: std.int = 1;

print("Enter max:");
val max: std.int = input_int();

while (index <= max) {
  fizzbuzz(index);
  index = index + 1;
}

fun fizzbuzz(x: std.int) {
  if (x % 5 == 0 && x % 3 == 0) {
    print("FizzBuzz");
  } else if (x % 5 == 0) {
    print("Buzz");
  } else if (x % 3 == 0) {
    print("Fizz");
  } else {
    print(int_to_str(x));
  }
}

